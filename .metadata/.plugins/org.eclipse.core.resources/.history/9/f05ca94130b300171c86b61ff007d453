/*
 * java继承
 * */
public class Jicheng extends anamil {

	public void bork(){
		System.out.println("谁在叫？狗在叫");
		super.bork();
	}
	public void sleep(){
		System.out.println("狗在睡觉");
		super.sleep();
	}
	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Jicheng jc=new Jicheng();
		jc.bork();
		jc.sleep();
		
	}

}
class anamil{
	public void bork(){
		System.out.println("叫声");
	}
	public void sleep(){
		System.out.println("睡觉");
	}
}
